{
  "private": true,
  "name": "tw0517tw.github.io",
  "description": "tw0517tw personal site",
  "license": "UNLICENSED",
  "author": "tw0517tw",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build && cpy CNAME public/ && gh-pages -d public -b master -m \"Auto-generated commit [ci skip]\"",
    "dev": "yarn develop",
    "develop": "gatsby develop",
    "lint:js": "eslint .",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "gatsby": "^2.28.2",
    "gatsby-plugin-styled-components": "^3.6.0",
    "graphql": "^14.5.8",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.6"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "babel-plugin-styled-components": "^1.12.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.15.0",
    "eslint-config-yoctol": "^0.25.1",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "prettier-package-json": "^2.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true
  }
}

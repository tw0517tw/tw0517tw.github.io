{
  "private": true,
  "name": "tw0517tw.github.io",
  "description": "tw0517tw personal site",
  "license": "UNLICENSED",
  "author": "tw0517tw",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build && cpy CNAME public/ && gh-pages -d public -b master -m 'Auto-generated commit [ci skip]'",
    "dev": "yarn develop",
    "develop": "gatsby develop",
    "lint:js": "eslint .",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.8.0",
    "gatsby": "^2.0.0",
    "gatsby-plugin-styled-components": "^3.0.1",
    "graphql": "^0.13.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "styled-components": "^4.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "cpy-cli": "^2.0.0",
    "eslint": "^5.6.1",
    "eslint-config-yoctol": "^0.18.2",
    "eslint-plugin-graphql": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.3.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.1.3",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.2",
    "prettier-package-json": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true
  }
}
